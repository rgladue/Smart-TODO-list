<body>
  <!-- The Modal -->
<div id="myModal" class="modal">

  <!-- Modal content -->
  <div class="modal-content">
    <!-- <span class="close">&times;</span> -->
    
  </div>

</div>
<!--navbar-->
  <nav class="navbar">
    
    <p style="color: rgb(77, 77, 77); width: 140px;"><i class="fa-solid fa-user"></i> Welcome!</p>
    <h1 id="title">SmartListr</h1>
    <span>
      <button type="button" class="navbar-login">Login</button>
      <button type="button" class="navbar-register" onclick= >Register</button>
    </span>
  </nav>
  <!--new todo list submission form-->
  <form class="input-form" method="POST" action="/">
    <p>New To-Do:</p>
    <br>
    <input type="text" class="text-field">
    <br>
    <input type="submit" id="add-btn" value="Add">
  </form>
  <!--main content with listing categories-->
  <div class="list-container">
    <div class="movie-container">
      <h6>MOVIES</h6>
      <ul id="movies">
        
      </ul>
    </div>
    <div class="book-container">
      <h6>BOOKS</h6>
      <ul id="books"></ul>
    </div>
    <div class="food-container">
      <h6>FOOD</h6>
      <ul id="food"></ul>
    </div>
    <div class="shop-container">
      <h6>SHOPPING</h6>
      <ul id="shop"></ul>
    </div>
  </div>
  <!--footer content -->
  <footer class="footer">
