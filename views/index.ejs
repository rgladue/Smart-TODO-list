<!DOCTYPE html>
<html lang="en">

<head>
  <title>Smart ToDo List</title>

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />
  <link href="https://fonts.googleapis.com/css2?family=Bungee&family=Lobster&family=Major+Mono+Display&family=Source+Sans+Pro:ital,wght@0,300;0,600;1,300&family=Wire+One&display=swap" rel="stylesheet">
  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
</head>

<body>
  <nav class="navbar">
    <p>Hello, User Name!</p>
    <h1 id="title">SmartList</h1>
    <span>
      <button type="button" class="navbar-login">Login</button>
      <button type="button" class="navbar-register">Register</button>
    </span>
  </nav>
  <!--new todo list submission form-->
  <form class= input-form method="POST" action="/">
    <p>New To-Do:</p>
    <br>
    <input type="text" class="text-field">
    <br>
    <input type="submit" id="add-btn" value="Add">
  </form>
  <!--main content with listing categories-->
  <div class="list-container">
    <div class="movie-container">
      <p>MOVIES</p>
    </div>
    <div class="book-container">
      <p>BOOKS</p>
    </div>
    <div class="food-container">
      <p>FOOD</p>
    </div>
    <div class="shop-container">
      <p>SHOP</p>
    </div>
  </div>
  <footer class="footer">
    <p>links</p>
  </footer>
</body>

</html>